<?xml version="1.0" encoding="utf-8"?>
<openerp>
	<data>
		<record model="ir.ui.view" id="q_task_form2">
			<field name="name">q_task_form2</field>
			<field name="model">project.task</field>
			<field name="inherit_id" ref="project.view_task_form2"/>
			<field name="arch" type="html">
				<xpath expr="//field[@name='planned_hours']" position="after">
					<field name="module_id"/>
				</xpath>
			</field>
		</record>

		<record model="ir.ui.view" id="q_project_issue_form">
			<field name="name">q_project_issue_form</field>
			<field name="model">project.issue</field>
			<field name="inherit_id" ref="project_issue.project_issue_form_view"/>
			<field name="arch" type="html">
				<xpath expr="//sheet/group//group[1]/div" position="after">
					<field name="module_id"/>
				</xpath>
			</field>
		</record>
	</data>
</openerp>